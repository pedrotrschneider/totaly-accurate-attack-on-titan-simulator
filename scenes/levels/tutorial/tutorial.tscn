[node name="DA9" parent="DialogueBoxes" instance=ExtResource( 9 )]
transform = Transform( 1, 0, 0, 0, 1, 0, 0, 0, 1, -4, 20, -352 )
visible = true
text = "Look! That titan is attacking the green tower! Let's not panic, you can easily dispose of him with your blades. Use your 3D manouver system to get close to its neck and strike it by pressng [E]."

[node name="CollisionShape" parent="DialogueBoxes/DA9" index="0"]
transform = Transform( 1, 0, 0, 0, 1, 0, 0, 0, 1, 0, 5, 4 )

[node name="DA10" parent="DialogueBoxes" instance=ExtResource( 9 )]
transform = Transform( 1, 0, 0, 0, 1, 0, 0, 0, 1, -4, 20, -386 )
visible = true
text = "Awesome! But you are going to face a great variety of titans, some much larger than others, like this 20 meters monster. Dealing with this one is as easy as the last, he is only taller. Try to use the orange tower to propell yourself upward to get a better angle to strike his neck!"

